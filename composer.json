{
    "name": "avoutic/web-framework",
    "description": "web-framework is a secure-by-default PHP framework for quickly making PHP sites and APIs",
    "type": "library",
    "keywords": [
        "framework",
        "security"
    ],
    "homepage": "https://web-framework.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Avoutic",
            "email": "avoutic@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/avoutic/web-framework/issues",
        "source": "https://github.com/avoutic/web-framework",
        "docs": "https://web-framework.com"
    },
    "require": {
        "php": "^8.2",
        "latte/latte": "^3.0",
        "slim/psr7": "^1.6",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-message": "^1.1",
        "php-di/php-di": "^7.0",
        "slim/slim": "^4.11",
        "odan/session": "^6.1",
        "slim/http": "^1.3",
        "guzzlehttp/guzzle": "^7.9",
        "nesbot/carbon": "^3.9",
        "psr/log": "^3.0",
        "monolog/monolog": "^3.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/verify": "^3.0",
        "haydenpierce/class-finder": "^0.5.3"
    },
    "autoload": {
        "psr-4": {
            "WebFramework\\": "src/",
            "WebFramework\\Actions\\": "actions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebFramework\\PHPStan\\": "phpstan/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": false,
            "php-http/discovery": false
        }
    }
}
